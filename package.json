{
  "name": "nodejs-vscode-debug",
  "version": "1.0.0",
  "description": "Node.jsのWebアプリケーションをVSCodeでリモートデバッグする",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc --sourceMap false --removeComments true",
    "build-debug": "tsc --build",
    "clean": "tsc --build --clean",
    "debug": "npm run build-debug && npm run debug:compose",
    "debug:compose": "docker-compose up --build",
    "start": "node .",
    "start-debug": "node --inspect=0.0.0.0 .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/walk8243/nodejs-vscode-debug.git"
  },
  "keywords": [],
  "author": "walk8243",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/walk8243/nodejs-vscode-debug/issues"
  },
  "homepage": "https://github.com/walk8243/nodejs-vscode-debug#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.37",
    "typescript": "^4.2.3"
  }
}
